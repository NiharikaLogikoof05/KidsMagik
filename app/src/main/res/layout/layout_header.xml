<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/app_bar_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#f3cb25">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:contentInsetLeft="0dp"
            android:contentInsetStart="0dp"
            app:contentInsetLeft="0dp"
            app:contentInsetStart="0dp"
            android:contentInsetRight="0dp"
            android:contentInsetEnd="0dp"
            android:paddingTop="0dp"
            android:paddingBottom="0dp"
            app:contentInsetRight="0dp"
            app:contentInsetEnd="0dp"
            android:layout_margin="0dp"
            app:contentInsetStartWithNavigation="0dp"
            app:contentInsetEndWithActions="0dp"
            android:layout_gravity="center"
            android:gravity="center"
            android:elevation="30dp"
            android:translationZ="5dp"
            android:clipChildren="false"
            android:padding="0dp">
            <RelativeLayout
                android:id="@+id/rr_header"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_50sdp">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true">
                    <ImageView
                        android:id="@+id/ivmenu"
                        android:layout_width="@dimen/_20sdp"
                        android:layout_height="@dimen/_20sdp"
                        android:src="@drawable/sidemenu"
                        android:layout_marginLeft="@dimen/_15sdp"
                        android:layout_centerVertical="true"/>
                    <ImageView
                        android:id="@+id/ivback"
                        android:visibility="gone"
                        android:layout_width="@dimen/_20sdp"
                        android:layout_height="@dimen/_20sdp"
                        android:src="@drawable/back"
                        android:layout_marginLeft="@dimen/_15sdp"
                        android:layout_centerVertical="true"/>
                <LinearLayout
                    android:id="@+id/layprofile"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:gravity="center"
                    android:layout_marginRight="@dimen/_60sdp"
                    android:layout_marginLeft="@dimen/_60sdp">
                    <ImageView
                        android:layout_width="@dimen/_25sdp"
                        android:layout_height="@dimen/_25sdp"
                        android:src="@drawable/user_profile"/>
                    <TextView
                        android:id="@+id/tvdash"
                        android:textFontWeight="500"
                        android:fontFamily="@font/poppinsregular_400"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/_12sdp"
                        android:text="Hello,"
                        app:textAllCaps="true"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:inputType="textCapCharacters"
                        android:textAllCaps="true"
                        android:textColor="#FFEB3B" />
                    <TextView
                        android:id="@+id/tvusername"
                        android:layout_below="@id/tvdash"
                        android:textFontWeight="900"
                        android:fontFamily="@font/poppinsregular_400"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/_12sdp"
                        android:text="Mayank Shekhar"
                        android:layout_marginLeft="@dimen/_2sdp"
                        app:textAllCaps="true"
                        android:inputType="textCapCharacters"
                        android:textAllCaps="true"
                        android:textColor="#090a0e" />
                </LinearLayout>
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/_10sdp">
                        <RelativeLayout
                            android:id="@+id/laycart"
                            android:layout_width="@dimen/_25sdp"
                            android:layout_height="@dimen/_20sdp">
                            <ImageView
                                android:id="@+id/ivbasket"
                                android:layout_width="@dimen/_20sdp"
                                android:layout_height="@dimen/_20sdp"
                                android:src="@drawable/kart"
                                android:layout_marginRight="@dimen/_5sdp"
                                android:layout_centerVertical="true"/>
                            <RelativeLayout
                                android:layout_width="@dimen/_10sdp"
                                android:layout_height="@dimen/_10sdp"
                                android:layout_alignParentRight="true"
                                android:background="@drawable/circle_black">
                                <TextView
                                    android:id="@+id/tvcartno"
                                    android:textFontWeight="300"
                                    android:fontFamily="@font/poppinsregular_400"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textSize="@dimen/_5sdp"
                                    android:text="12"
                                    android:layout_centerInParent="true"
                                    android:textColor="#FFEB3B" />
                            </RelativeLayout>
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_toRightOf="@id/laycart"
                            android:layout_width="@dimen/_25sdp"
                            android:layout_height="@dimen/_20sdp">
                            <ImageView
                                android:id="@+id/ivnatification"
                                android:layout_width="@dimen/_20sdp"
                                android:layout_height="@dimen/_20sdp"
                                android:src="@drawable/notification"
                                android:layout_marginLeft="@dimen/_5sdp"
                                android:layout_centerVertical="true"/>
                            <RelativeLayout
                                android:layout_width="@dimen/_10sdp"
                                android:layout_height="@dimen/_10sdp"
                                android:layout_alignParentRight="true"
                                android:background="@drawable/circle_black">
                                <TextView
                                    android:id="@+id/tvnotino"
                                    android:textFontWeight="300"
                                    android:fontFamily="@font/poppinsregular_400"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textSize="@dimen/_5sdp"
                                    android:text="10"
                                    android:layout_centerInParent="true"
                                    android:textColor="#FFEB3B" />
                            </RelativeLayout>
                        </RelativeLayout>
                    </RelativeLayout>
                </RelativeLayout>
            </RelativeLayout>
        </androidx.appcompat.widget.Toolbar>
    </com.google.android.material.appbar.AppBarLayout>
</RelativeLayout>



